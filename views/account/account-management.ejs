    <% if (title) { %>
        
        <h1 id="account-h"><%= title %></h1>
        <% } else {
          res.redirect('/')
        } %>
        <%- messages() %>

        <% if (errors) { %>
            <ul class="notice2">
           <% errors.array().forEach(error => { %>
             <li><%= error.msg %></li>
          <%  }) %>
           </ul>
          <% } %>
          <div id="accountMg">
          <h2>Welcome <%= locals.accountData.account_firstname %></h2>
          <h2>You're logged in</h2><br>
          <p><a class="invLinks" href="/account/edit">Edit Account Information</a></p>
          <% if (locals.accountData.account_type === "Admin" || locals.accountData.account_type == "Employee") { %>
            <hr>
            <h3>Inventory Management</h3>
            <p><a class="invLinks" href="/inv/">Inventory Management</a></p>
            <hr>
            <% } %>
          </div>

